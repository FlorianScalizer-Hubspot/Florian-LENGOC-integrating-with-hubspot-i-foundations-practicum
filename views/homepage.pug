doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/css/style.css')
  body
    div.container
      h1 Custom Objects List
      
      p
        a(href='/update-cobj') Add to this table
      
      table
        thead
          tr
            th Name
            th Color
            th Size
        tbody
          if data && data.length > 0
            each object in data
              tr
                td= object.properties.name
                td= object.properties.color
                td= object.properties.size
          else
            tr
              td(colspan='3') No objects found